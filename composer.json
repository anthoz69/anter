{
    "name": "anthoz69/anter",
    "description": "This package is collage helper function and class for develop laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthoz69",
            "email": "st.anthoz69@gmail.com",
            "homepage": "https://itthipat.me"
        }
    ],
    "homepage": "https://github.com/anthoz69/anter",
    "keywords": ["Laravel", "Anter", "Anthoz69"],
    "require": {
        "illuminate/config": "^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^6.0 || ^7.0 || ^8.0",
        "intervention/image": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "~3.8.4 || ^4.0 || ^5.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Anthoz69\\Anter\\": "src/"
        },
        "files": [
            "src/Helpers/route.php",
            "src/Helpers/video.php",
            "src/Helpers/currency.php",
            "src/Helpers/time.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Anthoz69\\Anter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Anthoz69\\Anter\\AnterServiceProvider"
            ],
            "aliases": {
                "Anter": "Anthoz69\\Anter\\Facades\\Anter",
                "AnterStore": "Anthoz69\\Anter\\Facades\\AnterStore",
                "AnterImg": "Anthoz69\\Anter\\Facades\\AnterImg"
            }
        }
    }
}
